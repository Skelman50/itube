extends layouts/main
include mixins/videoBlock

block content
  .search__header
    h3 Searching for: #{searchingBy}
  .search__videos
    if videosList.length === 0
      h5 No Videos Found
    each video in videosList
      +videoBlock({
        title: video.title,
        views: video.views,
        videoFile: video.videoFile,
        id: video.id
      })